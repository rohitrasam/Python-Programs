if __name__ == '__main__':
    arr = sorted(list(map(int, input().split())))
    print(arr)
    a = 0
    b = len(arr)-1

    while True:
        h = (a + b) // 2
        if arr[h] == 0:
            print('0 is at index {}'.format(h+1))
            break
        elif arr[h] > 0:
            b = h
        elif arr[h] < 0:
            a = h
